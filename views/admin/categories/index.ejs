<%- include('../../partials/header.ejs')%>
    <! --Para incluir partes de outro documento(usando quando a parte externa e reutilizada em outros códigos) -->

        <body>
            <%- include('../../partials/navbar.ejs')%>
                <div class="container">
                    <div class="card" id='form'>
                        <div class="card-header">
                            <h1>Categories </h1>
                        </div>
                        <div class="card-body">
                            <table class=" table table-bordered">
                                <thead>
                                    <tr>
                                        <th>Id</th>
                                        <th>Título</th>
                                        <th>Slug</th>
                                        <th>Ações</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                        
                                        <% categories.forEach(category => { %>
                                            <th><%= category.id%></th>
                                        <th><%= category.title%></th>
                                        <th><%= category.slug%></th>
                                        <th><% %></th>
                                        

                                        <% })%>
                                    </tr>
                                </tbody>
                            </table>
                    </div>
                    </div>
                </div>
        </body>
        <%- include('../../partials/footer.ejs')%>